BODY{
	padding:0;
	margin:0;
}
BODY TD {
	FONT-SIZE: 12px; 
	COLOR: #000; 
	FONT-FAMILY: arial, helvetica, sans-serif;
}
a{
	COLOR: #cc0000;
	text-decoration: underline;
}

a:hover{
	COLOR: #FF6600;
	text-decoration: none;
}
.mainLink {
	COLOR: #FFFFFF;
	TEXT-DECORATION: none;
	font-weight: bold;
	padding:0px 8 0 8;
}
.mainLinkActive {
	COLOR: #fff;
	TEXT-DECORATION: none;
	font-weight: bold;
	vertical-align: bottom;
	padding:2px 4 0 4;
}
.mainLinkActive td#left{
	width:10px;
	background-image: url(../images/mainTab_left.gif);
	background-repeat: no-repeat;
	background-position: left top;
}
.mainLinkActive td#right{
	width:10px;
	background-image: url(../images/mainTab_right.gif);
	background-repeat: no-repeat;
	background-position: right top;
}
.mainLinkActive td#mid{
	background-repeat: repeat-x;
	background-position: left top;
	text-align: center;
	vertical-align: middle;
	height:22px;
	background-color: #92231C;
}

.mainLinkActive a{
	COLOR: #fff;
	TEXT-DECORATION: none;
	font-weight: bold;
	padding-right: 3px;
	padding-left: 3px;
	display:blcok;
	width:100%;
}
.mainLinkActive a:hover{
	COLOR: #fff;
	TEXT-DECORATION: none;
	font-weight: bold;
	padding-right: 3px;
	padding-left: 3px;
}

.mainLink{
	text-align: center;
	width:16%;
}
.mainLink td#left{
	display:none;
}
.mainLink td#right{
	display:none;
}
.mainLink td#mid{
	text-align: center;
}
.mainLink a{
	COLOR: #FFFFFF;
	font-weight: bold;
	padding-right: 3px;
	padding-left: 3px;
	text-decoration: none;
	display:blcok;
	width:100%;
}
.mainLink a:hover{
	COLOR: #FFFF00;
	text-decoration: none;
}

