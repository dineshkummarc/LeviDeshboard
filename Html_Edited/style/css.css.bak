Table.dataGridTable {
	background-color: #FFFFFF;
	border:#666 solid 0px;
	border-collapse:collapse;
	margin:0px;
}
.dataGridTable TD{
	border-left:1px solid #D1DADE;
	border-right:1px solid #D1DADE;
	border-top:1px solid #666666;
	border-bottom:1px solid #666666;
	font-size: 12px;
}
td.dataGridHeader {	
	color: #000;
	font-weight: bold;
	background:url(../images/bgPattern.gif) repeat-x bottom;
	padding: 2px;
	margin:0px;
	vertical-align: middle;
	height: 20px;
}

.dataGridTable TR.activeRow TD{
	background-color:#C1C9DB;
	/*border:#E1F0FF solid 1px;*/
}
.dataGridTable Table TD{
	border:#fff solid 0px;
}
.dataGridTable textarea{	
	color: #505861;
	background-color: #FFFFFF;
	border: 1px solid #6A7580;
	width:100%;
}
.dataGridTable TABLE.subTable,.dataGridTable TABLE.subTable TD{
	border:#F0FCFF solid 1px;
	background-color:#fff;
	font-size:9px;
	padding:1px;
}
.dataGridTable .subTable TD.itemHead{
	background-color:#E5E5E5;
	color: #003399;
	text-transform:uppercase;
	font-size:9px;
}

.dataGridTable a{
	COLOR: #cc0000;
	text-decoration: none;
	padding:3px;
}

.dataGridTable a:hover{
	COLOR: #FF6600;
	text-decoration: underline;
}

